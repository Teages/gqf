{
  "name": "@gqfn/nuxt",
  "type": "module",
  "version": "0.0.2",
  "description": "A Nuxt binding for @gqfn/core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:prepare": "pnpm dev:prepare",
    "test": "pnpm test:prepare && vitest run && playwright test",
    "test:watch": "pnpm test:prepare && vitest watch",
    "test:types": "pnpm test:prepare && vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@gqfn/core": "workspace:*",
    "@nuxt/kit": "^3.12.4",
    "destr": "^2.0.3",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "ohash": "^1.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@playwright/test": "^1.46.0",
    "@pothos/core": "^4.2.0",
    "@types/node": "^22.2.0",
    "@types/ws": "^8.5.12",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.0",
    "get-port-please": "^3.1.2",
    "graphql-yoga": "^5.6.3",
    "nuxt": "^3.12.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29",
    "ws": "^8.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
