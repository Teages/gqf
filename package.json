{
  "name": "gqfn",
  "type": "module",
  "version": "",
  "packageManager": "pnpm@9.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqf.git"
  },
  "scripts": {
    "postinstall": "pnpm -C packages/core build",
    "lint": "eslint --cache . && pnpm --filter ./packages/** lint",
    "lint:fix": "eslint --cache --fix . && pnpm --filter ./packages/** lint:fix",
    "test": "pnpm --filter ./packages/** test",
    "test:types": "pnpm --filter ./packages/** test:types",
    "build": "pnpm docs:preview && pnpm --filter ./packages/** build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pothos/core": "^4.1.0",
    "@shikijs/vitepress-twoslash": "^1.12.1",
    "@teages/oh-my-graphql": "0.0.2",
    "@types/node": "^20.14.10",
    "eslint": "^9.9.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-scalars": "^1.23.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.2"
  },
  "resolutions": {
    "@teages/gqf": "workspace:*"
  }
}
